  >

<!ELEMENT BASEFONT EMPTY>
<!ATTLIST BASEFONT
  id          ID             #IMPLIED
  size        CDATA          #REQUIRED
  color       %Color;        #IMPLIED
  face        CDATA          #IMPLIED
  >

<!ELEMENT FONT (%inline;)*>
<!ATTLIST FONT
  %coreattrs;
